        "drupal/smtp": "^1.0",
        "drupal/social_simple": "^2.0",
        "drupal/stable": "^2.0",
        "drupal/tacjs": "*",
        "drupal/token_filter": "^2.1",
        "drupal/total_control": "^3.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/verf": "*",
