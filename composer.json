{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.26",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.4",
        "drupal/bootstrap": "^3.19",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/cas": "^1.5",
        "drupal/cas_attributes": "^1.0@beta",
        "drupal/chosen": "^2.7",
        "drupal/config_installer": "^1.8",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/content_moderation_notifications": "^3.0",
        "drupal/core": "^8.6.0",
        "drupal/crop": "^2.0",
        "drupal/devel": "^2.0",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/elasticsearch_connector": "^6.0",
        "drupal/entity_browser": "^2.1",
        "drupal/facets": "^1.3",
        "drupal/focal_point": "^1.0",
        "drupal/geolocation": "^3.0",
        "drupal/geolocation_nominatim": "^1.0@alpha",
        "drupal/inline_entity_form": "^1.0",
        "drupal/leaflet": "^1.7",
        "drupal/matomo": "^1.9",
        "drupal/media_entity_browser": "^2.0",
        "drupal/media_entity_pinterest": "^2.1",
        "drupal/metatag": "^1.8",
        "drupal/micro_menu": "^1.0@alpha",
        "drupal/micro_node": "^1.0@alpha",
        "drupal/micro_path": "^1.0@alpha",
        "drupal/micro_site": "^1.0@alpha",
        "drupal/micro_theme": "^1.0@alpha",
        "drupal/micro_user": "^1.0@alpha",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/piwik_reports": "^1.0",
        "drupal/redirect": "^1.3",
        "drupal/search_api": "^1.12",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/smtp": "^1.0",
        "drupal/token_filter": "^1.1",
        "drupal/webform": "^5.1",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches" : {
            "drupal/micro_theme": ["patches/modules/micro_theme/micro_theme-focal_point_issue.patch"]
            "drupal/cas_attributes": ["patches/modules/cas_attributes/cas_attributes-mapping_textarea.patch"],
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
