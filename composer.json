        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_custom_cache_tag": "^1.3",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_slideshow": "^5.0",
        "drupal/views_templates": "^1.5",
        "drupal/views_year_filter": "*",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2",
        "drupal/webform_xlsx_export": "^1.0",
        "drupal/xmlsitemap": "^1.0@RC",
        "drush/drush": "^12",
        "enyo/dropzone": "^5.8",
        "ruflin/elastica": "^7.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/devel": "^5",
        "drupal/devel_entity_updates": "^4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches" : {
            "drupal/core": {
                "translations_not_save": "https://www.drupal.org/files/issues/2020-02-12/translations_not_save_invalid_path-3101344-20.patch",
                "entity_system": "https://www.drupal.org/files/issues/2019-09-23/drupal-accommodate-numeric-query-parameter-values-3083077-2-D8.patch",
                "views": "https://www.drupal.org/files/issues/2021-12-18/unsupported-types-2952261-26.patch",
                "2894747: Views hardcodes exposed filter block form ID's": "https://www.drupal.org/files/issues/2024-05-25/2894747-10.2-125.patch",
                "2868014: Views Date Filter Datetime Granularity Option": "https://www.drupal.org/files/issues/2023-05-19/2868014-136.patch",
                "2719553: Log error when a TranslatableMarkup is created with a non-string input": "https://www.drupal.org/files/issues/2023-07-27/2719553-66.patch"
            },
            "drupal/tacjs": {
                "Patch Custom: modifie le service tacjs matomocloud avec celui de la version 1.14 de tarteaucitron": "patches/lmc_tracking_matomo_service_tacjs_1-14_v4.patch"
            },
            "drupal/social_simple" : {
                "3392860: Update fontawesom library to 6.x" : "https://git.drupalcode.org/project/social_simple/-/merge_requests/1.patch"
            },
            "drupal/field_group": {
                "1053174: hook_form_alter form state problems" : "https://www.drupal.org/files/issues/2021-10-15/1053174-48-field_group-D8-form-states.patch"
            },
            "drupal/ics_field" : [
                "https://www.drupal.org/files/issues/2022-05-24/ics_field-if_file_exists-3267032_3.patch"
            ],
            "drupal/micro_theme": [
                "patches/modules/micro_theme/micro_theme-focal_point_issue.patch"
            ],
            "drupal/siteimprove" : [
                "patches/modules/siteimprove/siteimprove_micro_node_urls.patch"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "htdocs/core": ["type:drupal-core"],
            "htdocs/libraries/{$name}": ["type:drupal-library"],
            "htdocs/modules/contrib/{$name}": ["type:drupal-module"],
            "htdocs/profiles/contrib/{$name}": ["type:drupal-profile"],
            "htdocs/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "htdocs/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": false
            }
        }
    }
}
