        "drupal/views_templates": "^1.1",
        "drupal/views_timelinejs": "^4.0",
        "drupal/webform": "^6.1@beta",
        "drupal/xmlsitemap": "^1.0@RC",
        "drush/drush": "^10.6",
        "mglaman/drupal-check": "^1.2",
        "mglaman/phpstan-drupal": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
	"enable-patching": true,    
        "patches" : {
            "drupal/micro_theme": ["patches/modules/micro_theme/micro_theme-focal_point_issue.patch"],
            "drupal/core": {
                "views_date_filter": "https://www.drupal.org/files/issues/2018-12-11/views_date_filter_granularity_2868014-60.patch",
                "node_preview_error": "https://www.drupal.org/files/issues/2019-06-24/node_preview_error-2914233-20.patch",
                "string_patch": "https://www.drupal.org/files/issues/2020-08-05/2719553-35.patch",
		        "user_system": "https://www.drupal.org/files/issues/2021-12-22/3255711-invalidate_session_on_destroy_session_manager-4.patch",
                "entity_system": "https://www.drupal.org/files/issues/2019-09-23/drupal-accommodate-numeric-query-parameter-values-3083077-2-D8.patch"
            },
            "drupal/paragraphs": ["https://www.drupal.org/files/issues/2018-11-09/2924774-46.patch"],
            "drupal/siteimprove" : [
                "patches/modules/siteimprove/siteimprove_micro_node_urls.patch"
            ],
            "drupal/rules" : ["https://www.drupal.org/files/issues/2021-01-28/email_textarea_via_typed_data.patch"],
            "drupal/ics_field" : ["https://www.drupal.org/files/issues/2022-05-24/ics_field-if_file_exists-3267032_3.patch", "patches/modules/ics_field/check_fileRef_before_load_file.patch"],
	    "vendor/dkd/php-cmis" : ["patches/vendor/dkd/php-cmis-client/patch.diff"],
	    "drupal/feeds" : ["https://www.drupal.org/files/issues/2022-04-22/feeds-renderRoot-issue-3261687-5.patch"],
	    "drupal/paragraphs_ee" : ["https://www.drupal.org/files/issues/2023-08-25/3383301-2.patch"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
