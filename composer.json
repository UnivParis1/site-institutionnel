{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.4",
        "drupal/anchor_link": "^2.5",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/bootstrap": "^3.19",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/captcha": "^1.1",
        "drupal/cas": "^1.7",
        "drupal/cas_attributes": "^2.0@beta",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_a11ychecker": "^2.0@alpha",
        "drupal/ckeditor_abbreviation": "^2.0",
        "drupal/ckeditor_balloonpanel": "^2.0@alpha",
        "drupal/ckeditor_liststyle": "^1.3",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_installer": "^1.8",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/content_moderation_notifications": "^3.0",
        "drupal/content_sync": "^2.1",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/crop": "^2.0",
        "drupal/date_popup": "^1.0",
        "drupal/devel": "^2.0",
        "drupal/dropzonejs": "^2.5",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/editor_file": "^1.4",
        "drupal/elasticsearch_connector": "^6.0@alpha",
        "drupal/entity_browser": "^2.1",
        "drupal/extlink": "^1.5",
        "drupal/facets": "^1.3",
        "drupal/facets_custom_label": "^1.1",
        "drupal/fakeobjects": "1.1",
        "drupal/field_group": "^3.0",
        "drupal/focal_point": "^1.0",
        "drupal/fontawesome": "^2.15",
        "drupal/geolocation": "^3.0",
        "drupal/geolocation_nominatim": "^1.0@alpha",
        "drupal/ics_field": "^1.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/leaflet": "^2.1",
        "drupal/link_attributes": "^1.8",
        "drupal/linkit": "^6.0@beta",
        "drupal/matomo": "^1.9",
        "drupal/matomo_reports": "^1.0",
        "drupal/media_entity_browser": "^2.0",
        "drupal/media_entity_pinterest": "^2.1",
        "drupal/menu_breadcrumb": "^1.11",
        "drupal/metatag": "^1.8",
        "drupal/micro_menu": "^1.0@alpha",
        "drupal/micro_node": "^1.0@alpha",
        "drupal/micro_path": "^1.0@alpha",
        "drupal/micro_site": "^1.0@alpha",
        "drupal/micro_sitemap": "^1.0@alpha",
        "drupal/micro_taxonomy": "^1.0@alpha",
        "drupal/micro_theme": "^1.0@alpha",
        "drupal/micro_user": "^1.0@alpha",
        "drupal/module_filter": "^3.1",
        "drupal/nbsp": "^2.0@alpha",
        "drupal/notification": "^1.0",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.3",
        "drupal/scheduler": "^1.4",
        "drupal/scheduler_content_moderation_integration": "^1.3",
        "drupal/search_api": "^1.12",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_elasticsearch_attachments": "^6.0",
        "drupal/similarterms": "^1.3",
        "drupal/siteimprove": "^1.8",
        "drupal/sitemap": "^1.5",
        "drupal/smtp": "^1.0",
        "drupal/social_simple": "^1.7",
        "drupal/token_filter": "^1.1",
        "drupal/total_control": "^3.0",
        "drupal/upgrade_status": "^3.11",
        "drupal/views_bootstrap": "^4.1",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_infinite_scroll": "^1.6",
        "drupal/views_templates": "^1.1",
        "drupal/webform": "^6.1",
        "drupal/xmlsitemap": "^1.0@RC",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.8",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches" : {
            "drupal/cas": [
                "https://www.drupal.org/files/issues/2020-06-14/2583325-39.patch"
            ],
            "drupal/core": {
                "views_date_filter": "https://www.drupal.org/files/issues/2018-12-11/views_date_filter_granularity_2868014-60.patch",
                "node_preview_error": "https://www.drupal.org/files/issues/2019-06-24/node_preview_error-2914233-20.patch",
                "string_patch": "https://www.drupal.org/files/issues/2020-08-05/2719553-35.patch",
                "translations_not_save": "https://www.drupal.org/files/issues/2020-02-12/translations_not_save_invalid_path-3101344-20.patch"
            },
            "drupal/easy_breadcrumb": {
                "truncate_title": "https://www.drupal.org/files/issues/2021-03-09/easy_breadcrumb-3202162-13.patch"
            },
            "drupal/ics_field": [
                "patches/modules/ics_field/ics_field-2852239-11.patch",
                "https://www.drupal.org/files/issues/2019-02-04/getting-error-when-edit-node-or-uninstalling-module-3030489.patch"],
            "drupal/micro_theme": [
                "patches/modules/micro_theme/micro_theme-focal_point_issue.patch"
            ],
            "drupal/entity_reference_revisions": [
                "https://www.drupal.org/files/issues/2020-06-09/3150084_2.patch"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
