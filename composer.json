        "drupal/flag": "^4.0@beta",
        "drupal/focal_point": "^1.0",
        "drupal/fontawesome": "^2.15",
        "drupal/force_users_logout": "^2.0",
        "drupal/geolocation": "^3.0",
        "drupal/gutenberg": "^2.0",
        "drupal/ics_field": "^2.0",
